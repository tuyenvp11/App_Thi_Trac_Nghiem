<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tuyenvp.appthitracnghiem_001">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/image_exam"
        android:label="@string/app_name"
        android:roundIcon="@drawable/image_exam"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppThiTracNghiem_001">
        <activity
            android:name="com.tuyenvp.appthitracnghiem_001.user.SignUpActivity"
            android:exported="true" />
        <activity
            android:name="com.tuyenvp.appthitracnghiem_001.user.SignInActivity"
            android:exported="true" />
        <activity
            android:name="com.tuyenvp.appthitracnghiem_001.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tuyenvp.appthitracnghiem_001.user.MainActivity"
            android:exported="true" />
        <activity
            android:name="com.tuyenvp.appthitracnghiem_001.slide.TestDoneActivity"
            android:exported="true" />
        <activity
            android:name="com.tuyenvp.appthitracnghiem_001.slide.ScreenSlidePagerActivity"
            android:exported="true" />
    </application>
</manifest>